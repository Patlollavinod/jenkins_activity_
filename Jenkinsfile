    stage('test') {
        sh 'echo hello'
    }
    stage('learning') {
        git url: https://github.com/Patlollavinod/jenkins_activity_'',
            branch: 'master'
    }
}
pipeline {
    agent any
    stages {
        stage('test') {
            steps {
                sh 'echo hello'
            }
        }
        stage('learning') {
            steps {
                git url:https://github.com/Patlollavinod/jenkins_activity_ '', 
                    branch: 'master'
            }
        }
    }
}
